	"versionCityJSON": "2.0",
	"description": "Extension for SIP Bridge Structure Model",
	"definitions": {
		"measureOrNil": {
			"type": "object",
			"properties": {
				  "value": { "type": ["number","null"] },
				  "uom": { "type": "string" }
			},
			"required": [ "value", "uom" ],
			"additionalProperties": false
		},
		"codeType":{
			"type":"object",
			"properties": {
				"code":{"type": "string"},
				"codeSpace":{"type": "string"}
			}
		},
		"stringOrNil":{
			"type":"object"	,
			"properties": {"value":{"type": ["string","null"]}		
			
			}
		},
		"numberOrNil":{
			"type":"object"	,
			"properties": {"value":{"type": ["number","null"]}		
			
			}
		},
		"booleanOrNil":{
			"type":"object"	,
			"properties": {"value":{"type": ["boolean","null"]}		
			
			}
		},
		"Resource":{
			"type":"object",
			"properties": {
				"sip:title":{
					"$ref": "bridge.ext.json#/stringOrNil"
				},				
